// Add Mega Bezel Params & Initial Prep
//      Add Mega Bezel parameters at the beginning of the list for easy access
//      Add Color Correction
//      Calculate Average Luminance and put in alpha channel
//      Debug Drawing of the resolution & screen scaling info
shader0 = ../bezel/shaders/HyperspaceMadness/hsm-prep-initial.slang
float_framebuffer0 = true

// Color Correction with Dogway's awesome Grade shader
shader1 = ../bezel/shaders/HyperspaceMadness/hsm-grade/hsm-grade.slang
alias1  = AvgLumPass

// De-Dithering - Mdapt
shader2 = ../bezel/shaders/HyperspaceMadness/hsm-mdapt/hsm-mdapt-pass0.slang
shader3 = ../bezel/shaders/HyperspaceMadness/hsm-mdapt/hsm-mdapt-pass1.slang
shader4 = ../bezel/shaders/HyperspaceMadness/hsm-mdapt/hsm-mdapt-pass2.slang
shader5 = ../bezel/shaders/HyperspaceMadness/hsm-mdapt/hsm-mdapt-pass3.slang
shader6 = ../bezel/shaders/HyperspaceMadness/hsm-mdapt/hsm-mdapt-pass4.slang
alias6 = refpass

shader7 = ../bezel/shaders/HyperspaceMadness/hsm-scalefx/hsm-scalefx-pass0.slang
filter_linear7 = false
scale_type7 = source
scale7 = 1.0
float_framebuffer7 = true
alias7 = scalefx_pass0

shader8 = ../bezel/shaders/HyperspaceMadness/hsm-scalefx/hsm-scalefx-pass1.slang
filter_linear8 = false
scale_type8 = source
scale8 = 1.0
float_framebuffer8 = true

shader9 = ../bezel/shaders/HyperspaceMadness/hsm-scalefx/hsm-scalefx-pass2.slang
filter_linear9 = false
scale_type9 = source
scale9 = 1.0

shader10 = ../bezel/shaders/HyperspaceMadness/hsm-scalefx/hsm-scalefx-pass3.slang
filter_linear10 = false
scale_type10 = source
scale10 = 1.0

shader11 = ../bezel/shaders/HyperspaceMadness/hsm-scalefx/hsm-scalefx-pass4.slang
filter_linear11 = false
scale_type11 = source
scale11 = 3

// GTU TV Processing
shader12 = ../bezel/shaders/HyperspaceMadness/hsm-gtu/hsm-gtu-pass1.slang
scale_type12 = source
scale12 = 1.0
float_framebuffer12 = true

shader13 = ../bezel/shaders/HyperspaceMadness/hsm-gtu/hsm-gtu-pass2.slang
scale_type_x13 = source
scale_x13 = 1.0
scale_type_y13 = source
scale_y13 = 1.0
filter_linear13 = false
float_framebuffer13 = true

// //NTSC passes added here so you can swap out with GTU
// shader12 = ../bezel/shaders/HyperspaceMadness/hsm-ntsc-adaptive/hsm-ntsc-pass1.slang
// scale_type12 = source
// scale_x12 = 4.0
// filter_linear12 = false
// float_framebuffer12 = true

// shader13 = ../bezel/shaders/HyperspaceMadness/hsm-ntsc-adaptive/hsm-ntsc-pass2.slang
// scale_type13 = source
// scale_x13 = 0.25
// filter_linear13 = false

// Linearize Color ----------------------------------------------------------------
shader14 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-guest-linearize.slang
filter_linear14 = false
scale_type14 = source
scale14 = 1.0
float_framebuffer14 = true
alias14 = LinearizePass

// Guest-Venom Pass ----------------------------------------------------------------
shader15 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-guest-linearize_scanlines.slang
filter_linear15 = true
scale_type15 = source
scale15 = 1
float_framebuffer15 = true

// Reduce resolution to a small consistent size so blur is consistent between different core resolutions
shader16 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-get-linearize-pass.slang
filter_linear16 = true
mipmap_input16 = true
scale_type16 = absolute
scale_x16 = 320
scale_y16 = 320

// Guest-Venom Pass ----------------------------------------------------------------
shader17 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-guest-blur_horiz.slang
// mipmap_input17 = true // This is commented out on purpose, was causing small white dots appearing on final image
filter_linear17 = true
scale_type17 = source
scale17 = 1
float_framebuffer17 = true

// Guest-Venom Pass ----------------------------------------------------------------
shader18 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-guest-blur_vert.slang
mipmap_input18 = true
filter_linear18 = true
scale_type18 = source
scale18 = 1
float_framebuffer18 = true
alias18 = GlowPass

// CRT Shader - Guest-Venom ----------------------------------------------------------------
//      Guest's Awesome Shader!!!
//      Has been slightly altered to use the same scaling as the rest of the shders for the bezel & reflection
shader19 = ../bezel/shaders/HyperspaceMadness/hsm-guest/hsm-crt-guest-dr-venom-with-scaling-glass.slang
mipmap_input19 = true
filter_linear19 = true
scale_type19 = viewport
scale19 = 1.0
float_framebuffer19 = true
wrap_mode19 = mirrored_repeat
alias19 = BR_CRTPass

// Tube Scale Curvature Mapping
// Write mapping into the pass, of the 4 channels, x & y are tube warped coord and z & w are outside tube mask and edge mask
shader20 = ../bezel/shaders/HyperspaceMadness/hsm-curvature-mapping-glass.slang
mipmap_input20 = true
filter_linear20 = true
scale_type20 = viewport
float_framebuffer20 = true
alias20 = BR_MappingPass

// Reduce Resolution  ----------------------------------------------------------------
//      Reduce the resolution to a small static size regardless of final resolution
//      Allows consistent look and faster at different final resolutions for blur
//      Mipmap option allows downscaling without artifacts
shader21 = ../bezel/shaders/HyperspaceMadness/hsm-linearize-crt.slang
mipmap_input21 = true
filter_linear21 = true
scale_type21 = absolute
float_framebuffer21 = true
// scale_x21 = 480
// scale_y21 = 270
// scale_x21 = 960
// scale_y21 = 540
scale_x21 = 800
scale_y21 = 600
wrap_mode21 = mirrored_repeat
float_framebuffer21 = true
alias21 = BR_MirrorLowResPass

// Add Blur for the Reflection (Horizontal) ----------------------------------------------------------------
shader22 = ../bezel/shaders/HyperspaceMadness/hsm-blur-outside-screen-horiz-glass.slang
mipmap_input22 = true
filter_linear22 = true
float_framebuffer22 = true
scale_type23 = source
wrap_mode22 = mirrored_repeat

// Add Blur for the Reflection (Vertical) ----------------------------------------------------------------
shader23 = ../bezel/shaders/HyperspaceMadness/hsm-blur-outside-screen-vert-glass.slang
filter_linear23 = false
scale_type23 = source
float_framebuffer23 = true
wrap_mode23 = mirrored_repeat
alias23 = BR_MirrorBlurredPass

// Reduce resolution ----------------------------------------------------------------
// Reduced to a very small amount so we can create a blur which will create a glow from the screen
//      Mipmap option allows smoother downscaling
shader24 = ../blurs/blur9x9.slang
mipmap_input24 = true
filter_linear24 = true
scale_type24 = absolute
float_framebuffer24 = true
scale_x24 = 128
scale_y24 = 128
wrap_mode24 = mirrored_repeat
alias24 = BR_MirrorReflectionDiffusedPass

// Add Diffused glow all around the screen ----------------------------------------------------------------
//      lurred so much that it's non directional
//      Mipmap option allows downscaling without artifacts
shader25 = ../blurs/blur9x9.slang
mipmap_input25 = true
filter_linear25 = true
scale_type25 = absolute
float_framebuffer25 = true
scale_x25 = 12
scale_y25 = 12
wrap_mode25 = mirrored_repeat
alias25 = BR_MirrorFullscreenGlowPass

// Bezel Generation & Reflection ----------------------------------------------------------------
//      Uses these previous passes:
//          BR_CRTPass
//          BR_MirrorBlurredPass
//          BR_MirrorReflectionDiffusedPass
//          BR_MirrorFullscreenGlowPass
//      Blur passes are in linearized color space, and CRT Pass already has gamma encoded in it
//
//      Uses these textures
            // BackgroundImage
            // BackgroundVertImage
            // TubeGlassImage
            // NightLightingImage
            // LEDImage
            // TopLayerImage
            // TopLayerVertImage

shader26 = ../bezel/shaders/HyperspaceMadness/hsm-mega-bezel-reflection-glass.slang
mipmap_input26 = true
filter_linear26 = true
scale_type26 = viewport
float_framebuffer26 = false

// Define how many shaders will be used from this list
shaders = 27

// Define textures to be used by the different passes
textures = "SamplerLUT1;SamplerLUT2;SamplerLUT3;BackgroundImage;BackgroundVertImage;TubeGlassImage;BezelImage;DecalImage;NightLightingImage;LEDImage;TopLayerImage;TopLayerVertImage;"

